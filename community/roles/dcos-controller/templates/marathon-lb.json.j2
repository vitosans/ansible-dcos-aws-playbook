{
  "volumes": null,
  "id": "/marathon-lb",
  "cmd": null,
  "args": [
    "sse",
    "-m",
    "http://marathon.mesos:8080",
    "--health-check",
    "--haproxy-map",
    "--group",
    "external"
  ],
  "user": null,
  "env": {
    "HAPROXY_SSL_CERT": "",
    "HAPROXY_SYSCTL_PARAMS": "net.ipv4.tcp_tw_reuse=1 net.ipv4.tcp_fin_timeout=30 net.ipv4.tcp_max_syn_backlog=10240 net.ipv4.tcp_max_tw_buckets=400000 net.ipv4.tcp_max_orphans=60000 net.core.somaxconn=10000"
  },
  "instances": {{ groups['tag_dcos_slave_public'] | length }},
  "cpus": 2,
  "mem": 1024,
  "disk": 0,
  "gpus": 0,
  "executor": null,
  "constraints": [["hostname", "UNIQUE", ""]],
  "fetch": null,
  "storeUrls": null,
  "backoffSeconds": 1,
  "backoffFactor": 1.15,
  "maxLaunchDelaySeconds": 3600,
  "container": {
    "docker": {
      "image": "{{ marathonlb_docker_image }}",
      "forcePullImage": false,
      "privileged": true,
      "network": "HOST"
    }
  },
  "healthChecks": [
    {
      "protocol": "HTTP",
      "path": "/_haproxy_health_check",
      "gracePeriodSeconds": 60,
      "intervalSeconds": 5,
      "timeoutSeconds": 2,
      "maxConsecutiveFailures": 2,
      "portIndex": 2,
      "ignoreHttp1xx": false
    }
  ],
  "readinessChecks": null,
  "dependencies": null,
  "upgradeStrategy": {
    "minimumHealthCapacity": 0.5,
    "maximumOverCapacity": 0.2
  },
  "acceptedResourceRoles": [
    "slave_public"
  ],
  "residency": null,
  "secrets": null,
  "taskKillGracePeriodSeconds": null,
